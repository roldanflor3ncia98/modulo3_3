  <!-- CONTACTANOS -->
  <div id="contactanos" class="col-md-6" action="/" method="post">
    <h2>Contactanos</h2>

    <form class="row g-3">
      <div class="col-md-6">
        <label for="nombre" class="form-label">Nombre</label>
        <input type="text" class="form-control" id="nombre" name="nombre">
      </div>

      <div class="col-md-6">
        <label for="apellido" class="form-label">Apellido</label>
        <input type="text" class="form-control" id="apellido" name="apellido">
      </div>

      <div class="col-12">
        <label for="email" class="form-label">Email</label>
        <input type="email" class="form-control" id="email" name="email" placeholder="ejemplo@email.com">
      </div>

      <div class="col-12">
        <label for="tel" class="form-label">Tel</label>
        <input type="text" class="form-control" id="tel" name="tel">
      </div>

      <div class="col-12">
        <label for="mensaje" class="form-label">Mensaje</label>
        <textarea class="form-control" id="mensaje" rows="3" name="mensaje"></textarea>
      </div>

      <div class="col-12">
        <button type="submit" class="btn btn-primary">Enviar</button>
      </div>
    </form>
    {{message}}
  </div>

</div>
